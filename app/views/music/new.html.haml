:javascript
  $(function(){
    $('#play').on('click', function(){
      playMusic();
    });
    $('#pause').on('click', function(){
      pauseMusic();
    });

    var t = T('sin');
    var playMusic = function(){
      console.log('t!');
      t.play();
    };
    var pauseMusic = function(){
      console.log('t...');
      t.pause();
    };
  });

%h1 Mathematical Music
.buttons
  = button_tag 'Play', id: 'play'
  = button_tag 'Pause', id: 'pause'
